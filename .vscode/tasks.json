{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: MinGW build active file",
      "command": "/usr/local/bin/x86_64-w64-mingw32-g++",
      "args": [
        "-fdiagnostics-color=always",
        "-O3",
        "-I",
        "${fileDirname}/../include",
        "-std=c++14",
        "-g",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}",
        "-lwinhttp",
        "-ladvapi32",
        "-lkernel32",
        "${file}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
